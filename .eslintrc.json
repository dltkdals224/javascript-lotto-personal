{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "airbnb-base",
    "eslint:recommended",
    "plugin:prettier/recommended",
    "plugin:cypress/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": false
    },
    "ecmaVersion": 12
  },
  "rules": {
    "no-unused-vars": "warn",
    "no-var": "error",
    "max-depth": ["error", 2],
    "no-console": "warn"
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },

  "plugins": ["import", "prettier"]
}
